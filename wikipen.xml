              nxslProcess(xmlParse(responsexml), nxslxauteur);
              var contentautor = xmlText(nxslxauteur);
              contentautor = contentautor.replace(/<p>/g, '')
                                        .replace(/<\/p>/g, '');
              
              contenttext = wiki2html(contenttext)
              contenttext = contenttext.replace(/&lt;/g, '<')
                                       .replace(/&gt;/g, '>')
                                       .replace(/&amp;/g, '&');
              _gel('wptitle').innerHTML = contenttitle;
              _gel('wpautor').innerHTML = contentautor;
              _gel('wptext').innerHTML  = contenttext;
          });
      });
